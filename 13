public class Main{
	public static void main(String args[]){
	    System.out.println("Oi mundo!");
	}
}

public class MyAge {
  int x = 21;

  public static void main(String[] args) {
    MyAge myObj = new MyAge();
    System.out.println("Minha idade e");
    System.out.println(myObj.x);
  }
}

public class MyBrothers {
  int x = 1;
  public static void main(String[] args) {
    MyBrothers myObj = new MyBrothers();
    System.out.println("Minha quantidade de irmao(s) e: " + myObj.x);
  }
}
